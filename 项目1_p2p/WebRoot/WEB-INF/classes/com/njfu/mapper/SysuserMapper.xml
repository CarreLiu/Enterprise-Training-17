<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.njfu.dao.SysuserDao">
	<sql id="sysuserColumn">
		id,
		userName,
		loginName,
		loginPassword
	</sql>
	<select id="selectByLoginNameAndLoginPassword" parameterType="com.njfu.form.SysuserForm" resultType="Sysuser">
		select
			<include refid="sysuserColumn"/>
		from
			sysuser
		where
			loginName=#{loginName} and loginPassword=#{loginPassword}
	</select>
	
</mapper>
